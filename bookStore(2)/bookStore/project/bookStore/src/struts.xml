<struts>
	<package name="bookStore" extends="struts-default" namespace="/" >
	    <action name="book_*" class="bookAction" method="{1}">       
	    </action>
	</package>
	<package name="bookStore1" extends="struts-default" namespace="/" >
	    <action name="order_*" class="orderAction" method="{1}">       
